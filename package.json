{
	"name": "@byterover/ct-cipher",
	"version": "0.3.2",
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">=9.14.0"
	},
	"description": "Core_Team-cipher (ct-cipher) - Memory-powered AI agent framework with real-time WebSocket communication, MCP integration, and composable patterns",
	"type": "module",
	"main": "dist/src/index.js",
	"module": "dist/src/index.js",
	"types": "dist/src/index.d.ts",
	"bin": {
		"cipher": "./dist/src/app/index.js"
	},
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"import": "./dist/src/index.js",
			"require": "./dist/src/index.js"
		},
		"./core": {
			"types": "./dist/src/core/index.d.ts",
			"import": "./dist/src/core/index.js",
			"require": "./dist/src/core/index.js"
		},
		"./core/index.js": {
			"types": "./dist/src/core/index.d.ts",
			"import": "./dist/src/core/index.js",
			"require": "./dist/src/core/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist/",
		"memAgent/cipher.yml"
	],
	"scripts": {
		"clean": "rm -rf dist",
		"prebuild": "pnpm run clean",
		"build": "pnpm exec tsc --noCheck && tsc-alias --resolve-full-paths",
		"postbuild": "cp instrument.mjs dist/ && chmod +x dist/src/app/index.js",
		"build:no-ui": "tsup && pnpm run copy-ui-dist",
		"build-ui": "cd src/app/ui && pnpm install && npx cross-env NODE_ENV=production BUILD_STANDALONE=true pnpm run build",
		"build-ui-only": "pnpm run build-ui && pnpm run copy-ui-dist",
		"copy-ui-dist": "npx tsx scripts/copy-ui-dist.ts",
		"prepare": "husky",
		"start": "node dist/src/app/index.js",
		"dev": "tsc --watch",
		"typecheck": "tsc --project tsconfig.typecheck.json",
		"precommit": "pnpm run lint && pnpm run typecheck && pnpm run format:check && pnpm run test:ci && pnpm run build",
		"test": "vitest run",
		"test:unit": "vitest run --exclude '**/integration/**' --exclude '**/*integration*.test.ts'",
		"test:integration": "SKIP_INTEGRATION_TESTS=false INTEGRATION_TESTS_ONLY=true vitest run",
		"test:ci": "pnpm run test:unit",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write \"src/**/*.ts\" \"*.{js,json,md,yml,yaml}\"",
		"format:check": "prettier --check \"src/**/*.ts\" \"*.{js,json,md,yml,yaml}\"",
		"prepublishOnly": "pnpm run build"
	},
	"keywords": [
		"mcp",
		"model-context-protocol",
		"memory",
		"vibe-coding",
		"byterover",
		"openai",
		"core-team-cipher",
		"ct-cipher"
	],
	"author": "BYTEROVER",
	"license": "Elastic-2.0",
	"devDependencies": {
		"@ai-sdk/openai": "^1.3.3",
		"@eslint/js": "^9.29.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.3.0",
		"@types/ws": "^8.5.13",
		"@typescript-eslint/eslint-plugin": "^8.15.0",
		"@typescript-eslint/parser": "^8.15.0",
		"cross-env": "^7.0.3",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"husky": "^9.1.6",
		"jest": "^29.7.0",
		"nodemon": "^3.1.7",
		"prettier": "^3.3.3",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tsc-alias": "^1.8.10",
		"tsup": "^8.3.5",
		"tsx": "^4.19.2",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.15.0",
		"vitest": "^2.1.4"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.32.1",
		"@libsql/client": "^0.14.0",
		"@model-context-protocol/sdk": "1.0.6",
		"@qdrant/js-client-rest": "^1.12.0",
		"ai": "^4.0.31",
		"chalk": "^5.3.0",
		"commander": "^12.1.0",
		"express": "^4.21.1",
		"ioredis": "^5.4.1",
		"neo4j-driver": "^5.26.0",
		"openai": "^4.73.0",
		"pg": "^8.13.1",
		"uuid": "^10.0.0",
		"winston": "^3.17.0",
		"ws": "^8.18.0",
		"yaml": "^2.6.1",
		"zod": "^3.23.8"
	}
}
